import importlib
import inspect
import os
import sys


sys.path.insert(0, os.getcwd())


import bot


for name in dir(bot):
    obj = getattr(bot, name)
    members = inspect.getmembers(obj)
    for name, obj in members:
        if name.startswith("_"):
            continue
        print(name)
        try:
            obj()
        except TypeError as ex:
            
            obj(
        except Exception as ex:
            bot.Errors.add(ex)
       
bot.Errors.show()
 