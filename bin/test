#!/usr/bin/env python3
# This file is placed in the Public Domain.
#
#


import importlib
import inspect
import os
import sys


sys.path.insert(0, os.getcwd())


import bot


cli = bot.CLI()


for name in dir(bot):
    obj = getattr(bot, name)
    members = inspect.getmembers(obj)
    for name, obj in members:
        if name.startswith("_"):
            continue
        if "__code__" in dir(obj):
            args = []
            for name in obj.__code__.co_varnames:
                if name == "obj":
                    args.append(bot.Object())
                elif name == "orig":
                    args.append(object.__repr__(cli))
                elif name == "txt":
                    args.append("yooo!")
            print(name, dir(obj.__code__))
            for name in dir(obj.__code__):
                item = getattr(obj.__code__, name)
                print(name, item)
            try:
                obj(*args)
            except TypeError as ex:
                bot.Errors.add(ex)
       

bot.Errors.show()
 